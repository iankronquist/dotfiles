[user]
	email = iankronquist@gmail.com
	name = Ian Kronquist
[core]
	editor = vim
	excludesfile = ~/.gitignore
[push]
	default = simple
[alias]
	net = log --graph --all --decorate
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
